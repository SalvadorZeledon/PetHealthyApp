{
  "expo": {
    "name": "PetHealthyApp",
    "slug": "PetHealthyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoPH.png",
    "scheme": "pethealtyapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/logoPerHealthyApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#E6F4FE"
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/logoPH.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.alejandrogt.PetHealthyApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b8376aac-a2b5-4343-8628-ecff777d61ed"
      }
    }
  }
}
